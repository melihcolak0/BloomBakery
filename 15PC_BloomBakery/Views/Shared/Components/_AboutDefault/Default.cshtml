@model List<ResultAboutDto>

@foreach(var item in Model)
{
    <div class="container-xxl py-6">
        <div class="container">
            <div class="row g-5">
                <div class="col-lg-6 wow fadeInUp" data-wow-delay="0.1s">
                    <div class="row img-twice position-relative h-100">
                        <div class="col-6">
                            <img class="img-fluid rounded" src="@item.ImageUrl" alt="">
                        </div>
                        <div class="col-6 align-self-end">
                            <img class="img-fluid rounded" src="@item.ImageUrl2" alt="">
                        </div>
                    </div>
                </div>
                <div class="col-lg-6 wow fadeInUp" data-wow-delay="0.5s">
                    <div class="h-100">
                        <p class="text-primary text-uppercase mb-2">// @item.Title</p>
                        <h1 class="display-6 mb-4">@item.SubTitle</h1>
                        <p>@item.Description</p>                        
                        <div class="row g-2 mb-4">
                            <div class="col-sm-6">
                                <i class="fa fa-check text-primary me-2"></i>@item.Feature1
                            </div>
                            <div class="col-sm-6">
                                <i class="fa fa-check text-primary me-2"></i>@item.Feature2
                            </div>
                            <div class="col-sm-6">
                                <i class="fa fa-check text-primary me-2"></i>@item.Feature3
                            </div>
                            <div class="col-sm-6">
                                <i class="fa fa-check text-primary me-2"></i>@item.Feature4
                            </div>
                        </div>
                        <a class="btn btn-primary rounded-pill py-3 px-5" href="">Daha fazlası</a>
                    </div>
                </div>
            </div>
        </div>
    </div>
}
